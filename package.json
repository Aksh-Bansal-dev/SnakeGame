{
  "name": "snakegame",
  "version": "1.0.0",
  "description": "The classic snake game(no fancy graphics).",
  "main": "main.js",
  "scripts": {
    "compile": "tsc",
    "dev": "tsc && electron .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aksh-Bansal-dev/SnakeGame.git"
  },
  "author": "Aksh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aksh-Bansal-dev/SnakeGame/issues"
  },
  "homepage": "https://github.com/Aksh-Bansal-dev/SnakeGame#readme",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.5",
    "typescript": "^4.2.4"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "snakegame"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
